5 {BUBBLE SORT OF SCREEN}
10 PROC BUBBLE();
15 BEGIN I=0;
20   LOOP:J=I;
25   LOOPA:IF SCREEN[J]>SCREEN[J+1] THEN
28   BEGIN
30     TEMP=SCREEN[J];
31     SCREEN[J]=SCREEN[J+1];
32     SCREEN[J+1]=TEMP;
35     IF J=0 THEN GOTO OK;
40     J=J-1;GOTO LOOPA
42   END;
45   OK:I=I+1;IF I<255 THEN GOTO LOOP
60 END
