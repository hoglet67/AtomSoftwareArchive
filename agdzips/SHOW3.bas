     6*NOMON
     7*LOAD DISOLV
    10C=#100;P=#2800
    20CLEAR4
    30DO
    40FOR N=0 TO NNNNN
    45*CWD /AGD
    50GOSUB (1000+N)
    60X=0;DO X=X+1;U.P?X=47
    70$C="CWD "
    80$C+LEN(C)=$P
    90C?(X+4)=13
   100LINK #FFF7;
   110$C="LOAD "
   120$C+LEN(C)=$P+X+1
   130$C+LEN(C)=" 6000"
   140LINK #FFF7;
   150LINK #4000
   160FOR I=1TO120;WAIT;NEXT
   170NEXT
   180UNTIL 0
   999REM SCREENS APPENDED DURING BUILD
